{
  "version": 2,
  "name": "node-js",
  "builds": [
    {
      "src": "frontend/build/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "backend/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/build/$1"
    }
  ],
  "regions": ["sfo1"]
}
